generator client {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
} 

model Employee {
  id  Int @id @default(autoincrement())
  name String
  email String @unique
  role Role
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Role {
  ADMIN
  ENGINEER
  INTERN
}
